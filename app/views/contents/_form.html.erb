<div class="form">
  <%= form_with(model: content, local: true) do |form| %>
    <%= form.text_field :name, placeholder:"nickname" %>
    <%= form.text_field :images, placeholder:"Image_url" %>
    <%= form.text_area :text, placeholder:"TEXT", rows:"10",cols:"30" %>
    <%= form.submit "SEND",class:"send" %>
  <% end %>
</div>